terraform {
  required_version = "1.9.1"

  backend "s3" {
    bucket         = "s3Name"
    key            = "terraform.state"
    region         = "us-east-1"
    dynamodb_table = "dynamodbName"

  }
  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 5.0"
    }
  }
}


